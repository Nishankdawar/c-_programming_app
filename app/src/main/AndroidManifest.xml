<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.davdayanandvihar.dav.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity android:name="net.davdayanandvihar.dav.myapplication.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="net.davdayanandvihar.dav.myapplication.Game" />
        <activity android:name="net.davdayanandvihar.dav.myapplication.learn" />
        <activity android:name="net.davdayanandvihar.dav.myapplication.LearnTopicSelector" />
        <activity android:name="net.davdayanandvihar.dav.myapplication.QuizTopicSelector" />
        <activity android:name="net.davdayanandvihar.dav.myapplication.forum"></activity>
    </application>

</manifest>